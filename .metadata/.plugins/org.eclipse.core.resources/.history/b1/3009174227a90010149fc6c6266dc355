package day8;

public class Recursion {

	public static int[] f = new int[100];
	public static int fibo(int n) {
		
		if(f[n] != 0) {
			return f[n];
		}
		if(n <= 2) {
			return 1;
 		}
		System.out.println("tÃ­nh f" + n);
		f[n] = fibo(n - 1) + fibo(n - 2);
		return f[n];
	}
	public static void main(String[] args) {
		System.out.print(fibo(6));
	}
}
