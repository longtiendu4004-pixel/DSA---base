package day12;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Nhập vào số a : ");
		int a = sc.nextInt();
		System.out.print("Nhập vào số b : ");
		int b = sc.nextInt();
		System.out.print("Tổng 2 số a và b : " + a + b);
	}
}
