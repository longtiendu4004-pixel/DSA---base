package day8;

public class Recursion {
	public static void printElement(int[] arr, int index) {
		if(index < 0 || index >= arr.length) {
			return;
		}
		System.out.print(arr[index] + " ");
		printElement(arr, index - 1);
	}
	public static int fibonaci(int n) {
		System.out.println("tính " + "f" + n);
		if(n <= 2) {
			return 1;
		}
		
		int f = fibonaci(n-2) + fibonaci(n-1);
		return f;
		
	}
	public static int[] f = new int[100];
	public static int fibo2(int n) {
		if(f[n] != 0) {
			return f[n];
		}
		if(n <= 2) {
			f[1] = 1;
			f[2] = 1;
			return 1;
 		}
		System.out.println("tính " + "f" + n);
		f[n] = fibo2(n - 1) + fibo2(n - 2);
		return f[n];
	}
	public static void main(String[] args) {
		System.out.print(fibo2(10));
	}
}
