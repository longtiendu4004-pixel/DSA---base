package next.neetcode;

public class Linkedlist {
	public Node head;
	
	public Linkedlist(Node head) {
		this.head = head;
	}
	public void printLinkedList() {
		if(head == null) {
			System.out.print("List is empty");
		}
		Node curr = head;
		while(curr != null) {
			System.out.print(curr.data + " ");
			curr = curr.next;
			if(curr.next != null) {
				System.out.print(" --> ");
			}
		}
	}
}
