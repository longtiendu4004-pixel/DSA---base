package next.neetcode;

public class bai1{
	// tên của class trùng với tên file
	public static void main(String[] args) {
		System.out.print("Đây là hàm in ra màn hình");
		System.out.print("My name is Long");
		//System : hệ thống
		//out : output
		// print: in
		//println : in rồi xuống dòng
		
	}
}
